.login-container .loginform-container {
	text-align: center;
	flex: 1 1 60%;
	background-color: white;
	background-color: rgba(255, 255, 255, .8);
	display: flex;
	flex-direction: column;
} /* //.loginform-container */
.login-container .login-instructions .log-inscontent h5, 
.login-container .forgetpass-instructions .fp-inscontent h5 {
	font-size: 1rem;
	margin-bottom: 1rem;
}
.login-container .login-instructions .log-inscontent p, 
.login-container .forgetpass-instructions .fp-inscontent p {
	margin: 0;
	margin-bottom: .8rem;
}
.login-container .login-instructions .log-inscontent .log-instructions, 
.login-container .forgetpass-instructions .fp-inscontent .fp-instructions {
	margin: 0;
	padding: 0 20px 0 30px;
} /* //.fp-instructions */
.login-container .login-instructions .log-inscontent .log-instructions li, 
.login-container .forgetpass-instructions .fp-inscontent .fp-instructions li {
	margin-bottom: .5rem;
}

.login-container .loginform-container .login-header {
	text-align: left;
	padding: 30px 20px 10px;
} /* //.login-header */
.login-container .loginform-container .login-header .brand-logo {
	/*padding: 10px 0px;*/
} /* //.brand-logo */
.login-container .loginform-container .login-header .brand-logo img {
} /* //img */
.login-container .loginform-container .login-form {
	/*background-color: red;*/
	display: inline-block;
	margin: auto;
	width: 300px;
} /* //.login-form */
.login-container .loginform-container .login-form .form-footer {
	padding: 10px 0;
	text-align: center;
} /* //.form-footer */
.login-container .loginform-container .login-form.prelogins .form-footer {
	/*display: flex;
	justify-content: space-between;*/
}
.login-container .loginform-container .login-form.prelogins .form-footer .button {
	margin-bottom: 15px;
}
.ui-button {
	
} /* //.ui-button */

.language-bar {
	float: right;
	padding-top: 5px;
} /* //.language-bar */
.language-bar .lang-dropdown {
	position: relative;
} /* //.lang-dropdown */
.language-bar .lang-dropdown .btn-selectlang {
	font-size: 10px;
	/*margin-right: 10px;*/
	color: #333;
	/*background-color: red;*/
	white-space: nowrap;
	position: relative;
	padding-right: 18px;
	line-height: 12px;
	/*text-transform: uppercase;*/
} /* //.btn-selectlang */
.language-bar .lang-dropdown .btn-selectlang i {
	display: block;
	position: absolute;
	width: 14px;
	height: 14px;
	right: 0px;/*top: -1px;*/
	top: 0px;
	transition: all .4s ease;
	/*background-color: #aaa;*/
} /* //i */
.language-bar .lang-dropdown .btn-selectlang i::after {
	content: '';
	width: 8px;
	height: 8px;
	border: 2px solid #777;
	border-top: 0;border-right: 0;
	/*background-color: purple;*/
	display: block;
	position: absolute;
	left: 50%;top: 50%;
	margin-left: -4px;
	margin-top: -6px;
	transform: rotate(-45deg);
} /* //i::after */
.language-bar .lang-dropdown.langlist-showing .btn-selectlang i {
	transform: rotate(180deg);
} /* //.selectlang-selected */
.language-bar .lang-dropdown .btn-selectlang em {
	font-size: 14px;
	font-weight: 500;
	font-style: normal;
	color: #08c;
	color: $brand;
	text-transform: none;
} /* //em */
.language-bar .lang-dropdown .language-list {
	position: absolute;
	width: 140px;
	right: 0;
	/*box-shadow: 0 2px 50px rgba(0, 0, 0, .4);*/
	/*border: 1px solid #eee;*/
	/*background-color: rgba(0, 0, 0, 0.96);*/
	background-color: $brand;
	display: none;
	z-index: 2;
	top: 30px;
	/*max-height: 0;*/
	/*overflow: hidden;
	transition: transform .3s ease;
	transform: scaleY(0);
	transform-origin: top;*/
} /* //.language-list */
.language-bar .lang-dropdown.langlist-showing .language-list {
	/*display: block;*/
	/*max-height: 300px;*/
	/*transform: scaleY(1);*/
} /* //.language-list */
.language-bar .lang-dropdown .language-list .lst-languages {
	list-style: none;
	margin: 0;padding: 0;
	/*border: 1px solid #464646;*/
} /* //.lst-languages */
.language-bar .lang-dropdown .language-list .lst-languages li {
	margin: 0;padding: 0;
	/*border-bottom: 1px solid #464646;*/
	border-bottom: 1px solid #007cbb;
} /* //li */
.language-bar .lang-dropdown .language-list .lst-languages li a {
	font-size: 12px;
	padding: 8px 10px;
	display: block;
	padding-left: 32px;
	/*color: #777;*/
	color: white;
	color: rgba(255, 255, 255, .9);
	position: relative;
} /* //a */
.language-bar .lang-dropdown .language-list .lst-languages li a.language-selected {
	/*background-color: #DCEDC8;*/
	/*background-color: #00679a;*/
	background-color: darken($brand, 20%);
	font-weight: 600;
	/*color: #2E7D32;*/
	color: white;
} /* //.language-selected */
.language-bar .lang-dropdown .language-list .lst-languages li a.language-selected::before {
	content: '';
	width: 15px;
	height: 8px;
	/*border: 2px solid #2E7D32;*/
	border: 2px solid white;
	border-top: 0;border-right: 0;
	display: block;
	position: absolute;
	left: 8px;top: 10px;
	transform: rotate(-45deg);
} /* //a.language-selected::before */
.login-container .loginform-container .login-form .welcome-msg {
	/*display: none;*/
} /* //.welcome-msg */
.login-container .loginform-container .login-form .welcome-msg h5 {
	/*color: var(--brandcolor);*/
	padding-bottom: 20px;
	margin: 0;
	letter-spacing: 1px;
} /* //h5 */
.login-container .loginform-container .login-footer {
	
} /* //.login-footer */
.login-container .loginform-container .login-footer .copyrights {
	font-size: 9px;
	color: #aaa;
	margin: 0;
	padding: 10px 0;
} /* //.copyrights */
@media (min-width: 1200px) {
	.login-container .loginform-container .login-form.prelogins {
		width: 550px;
	}
}
@media (min-width:992px) {
	.login-container .adbanner-container {
		display: block;
		display: flex;
	}
	.login-container .forgetpass-instructions {
		display: block;
		display: flex;
	}
	.login-container .login-instructions {
		display: block;
		display: flex;
	}
	.login-container .loginform-container .login-form .form-footer {
		text-align: right;
	}
	/*.login-container .loginform-container .login-form.prelogins .form-controller  {
		max-width: 500px;
		margin: .7rem auto;
	}*/
}
@media (min-width:576px) {
	.login-container .loginform-container .login-form {
		width: 400px;
	}
}
@media (min-width:370px) {
	.login-container .loginform-container .login-form .form-controller::before {
		content: '';
	}
}
@media (max-height: 600px) {
	.login-container {height: 100%;}
}

