/* ----------------------------
	Export Data List :: Start
--------------------------- */
.dt-exports {
	// padding: 5px 15px;
	padding: 2px 0px;
	border-top: 1px solid rgba($black, .1);
	border-bottom: 1px solid rgba($black, .1);
	@include transition;
	text-align: center;
	em {
		font-style: normal;
		font-size: .7rem;
		display: inline-block;
		vertical-align: middle;
		position: relative;
		padding: .6rem 0;
		padding-right: 22px;
		color: $brand;
		cursor: pointer;
		span.chevron {
			width: 12px;
			height: 12px;
			display: block;
			position: absolute;
			right: 5px;top: 50%;
			margin-top: -6px;
			@include transition;
			&::after {
				content: '';
				width: 8px;
				height: 8px;
				border: 2px solid rgba($black, .5);/* #777 */
				border-top: 0;border-right: 0;
				margin-left: 2px;
				display: block;
				@include transform(rotate(-45deg));
			} /* //&::after */
		} /* //span.chevron */
	} /* //em */
	.linkbutton {
		// display: inline-block;
		display: block;
		vertical-align: middle;
		background: url('#{$images-icon-path}export-icons.png') no-repeat 5px -22*10+2px;
		// padding: .3rem;
		padding: 0 .3rem;
		padding-left: 30px;
		// padding: 0;
		// padding: .3rem 1rem;
		font-size: .75rem;
		@include border-radius(4px);
		@include opacity(0);
		visibility: hidden;
		height: 0;
		overflow: hidden;
		// @include transition(height .4s ease,);
		// @include transition(opacity .2s ease-out, all .4s ease);
		// @include transition-(opacity .2s ease-out, all .4s ease);
		@include transition-property(all, opacity);
		@include transition-duration(.15s);
		@include transition-timing-function(cubic-bezier(0.4,0.0,0.2,1));
		&::before {
			@include border-radius(4px);
		} /* //&::before */
		&:nth-child(1) {
			// @include transform(translateY(-1*25px));
		} /* //&:nth-child(1) */
		&:nth-child(2) {
			// @include transform(translateY(-1*25px));
			@include transition-delay(40ms, 0ms);
		} /* //&:nth-child(2) */
		&:nth-child(3) {
			// @include transform(translateY(-2*25px));
			@include transition-delay(80ms, 40ms);
		} /* //&:nth-child(3) */
		&:nth-child(4) {
			// @include transform(translateY(-3*25px));
			@include transition-delay(120ms, 80ms);
		} /* //&:nth-child(4) */
		&:nth-child(5) {
			// @include transform(translateY(-4*25px));
			@include transition-delay(160ms, 120ms);
		} /* //&:nth-child(5) */
		&:nth-child(6) {
			// @include transform(translateY(-5*25px));
			@include transition-delay(200ms, 160ms);
		} /* //&:nth-child(6) */
		b {
			font-weight: 500;
			font-size: 80%;
			display: inline-block;
			vertical-align: middle;
			margin-top: -2px;
			color: rgba($black, .8);
			text-transform: lowercase;
		} /* //b */
		&.exp-excel {
			background-position: 5px 2px;
		} /* //&.exp-excel */
		&.exp-pdf {
			background-position: 5px -22*2+2px;
		} /* //&.exp-pdf */
		&.exp-word {
			background-position: 5px -22*4+2px;
		} /* //&.exp-word */
		&.exp-xml {
			background-position: 5px -22*6+2px;
		} /* //&.exp-xml */
		&.exp-csv {
			// background-position: 5px -22*8+2px;
		} /* //&.exp-csv */
	} /* //.linkbutton */
	&:hover {
		// background: rgba($black, .04);
	} /* //&:hover */
	&.expdrop-showing {
		em span.chevron {@include transform(rotate(180deg));}
		.linkbutton {
			padding-top: .3rem;
			padding-bottom: .3rem;
			// padding-left: 30px;
			height: 28px;
			@include opacity(1);
			visibility: visible;
		} /* //.linkbutton */
	} /* //&.expdrop-showing */
} /* //.dt-exports */
@media (min-width: 768px) {
	.dt-exports {
		padding: 5px 15px;
		text-align: left;
		em {
			display: inline-block;
			padding: 0;
			padding-right: 10px;
			color: inherit;
			cursor: auto;
			span.chevron {display: none;}
		} /* //em */
		.linkbutton {
			padding: .3rem;
			display: inline-block;
			@include opacity(1);
			visibility: visible;
			height: auto;
			padding-left: 30px;
			@include transition(none);
		} /* //.linkbutton */
	} /* //.dt-exports */
}
/* ----------------------------
	Export Data List :: End
--------------------------- */

/* ----------------------------
	File Upload Input :: Start
--------------------------- */
.file-input {
	border: 1px solid rgba($black, .2);
	@include border-radius(8px);
	position: relative;
	@include transition(all .3s ease);
	cursor: pointer;
	background-color: rgba($white, .9);
	text-align: center;
	input[type="file"] {
		width: 0px;
		height: 0px;
		@include opacity(0);
		position: absolute;
		z-index: -1;
		overflow: hidden;
	} /* //input[type="file"] */
	label {
		display: block;
		// padding: 30px 15px;
		// padding: 20px 10px; 
		//@Rajat
		padding: 15px 10px;
		margin: 0;
		// @include transition(all .3s ease);
		h4 {
			margin: 0;padding: 0 0 5px;
			display: block;
			width: 100%;
			white-space: nowrap;
			text-overflow: ellipsis;
			overflow: hidden;
			// font-size: 1.2rem;
			//@Rajat
			font-size: .9rem;
		} /* //h4 */
		p {
			margin: 0;padding: 0;
			// font-size: .8rem;
			//@Rajat
			font-size: .7rem;
		} /* //p */
		&::before {
			content: '';
			border: 1px dashed rgba($black, .2);
			position: absolute;
			left: 10px;top: 10px;right: 10px;bottom: 10px;
			@include border-radius(8px);
			z-index: 0;
			@include transition(all .3s ease);
		} /* //&::before */
	} /* //label */
	&:hover {
		border-color: rgba($brand, .6);
		background-color: rgba($brand, .08);
		label::before {border-color: rgba($brand, .6);}
	} /* //&:hover */
	&.fileuploading {
		border-color: rgba($action-success, .6);
		background-color: rgba($action-success, .08);
		label::before {border-color: rgba($action-success, .6);}
	} /* //&.fileuploading */
} /* //.file-input */
