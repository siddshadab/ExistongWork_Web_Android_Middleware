.form-controller {
	padding: .7rem .7rem .8rem;
	margin-bottom: .7rem;
	text-align: left;
	position: relative;
	label {
		margin: 0;
		display: block;
		/*font-size: 12px;*/
		font-size: .8rem;
		text-transform: uppercase;
		font-weight: bold;
		color: rgba($black, .4);
		@include transition;
		sup {
			color: $action-warning;
			padding-left: 2px;
		} /* //sup */
	} /* //label */
	input, &>div>select {
		display: block;
		width: 100%;
		background-color: transparent;
		border: none;
		outline: none;
		font-size: .9rem;
		padding: .5rem 0;
		font-weight: 500;
	} /* //input */
	textarea {
		display: block;
		width: 100%;
		// background-color: #f0f0f0;
		background-color: transparent;
		border: none;
		outline: none;
		font-size: .9rem;
		padding: .5rem 0;
		font-weight: 500;
		resize: none;
	} /* //textarea */
	&>em {
		font-size: .7rem;
		font-style: normal;
		margin: 0;
		color: red;
		display: block;
		line-height: 1rem;
		height: 15px;
		position: absolute;
		left: 10px;right: 10px;bottom: -2px;
	} /* //em */
	&>u, &>div>u {
		background: rgba($black, .1);
		height: 2px;
		// height: .12rem;
		margin:0;padding:0;
		border: 0;
		display: block;
		position: relative;
		&::after {
			content: '';
			background: $brand;
			height: 100%;
			display: block;
			left: 30%;
			width: 0%;
			position: absolute;
			top:0;bottom:0;
			@include transition(all .3s ease);
		} /* //&::after */
	} /* //&>u, &>div>u */
	&>div {
		position: relative;
		&>select {
			padding-right: 1.1rem;
			@include appearance;
			&::-ms-expand {
				display: none;
			}
		} /* //&>select */
		&::after {
			content: '';
			width: 8px;
			height: 8px;
			border: 2px solid rgba($black, .5);/* #777 */
			border-top: 0;border-right: 0;
			/*background-color: purple;*/
			display: block;
			position: absolute;
			right: 5px;top: 50%;
			margin-left: -4px;
			margin-top: -6px;
			transform: rotate(-45deg);
		} /* //&::after */
	} /* //&>div */
	h6 {
		font-size: .9rem;
		padding: .5rem 0;
		font-weight: 500;
		margin: 0 0 2px;
		line-height: inherit;
	} /* //h6 */
	&.control-mandatory {
		label {
			&::before {
				content: '*';
				display: inline-block;
				color: $action-warning;
				padding-right: 3px;
			} /* //&::before */
			&.selection::before{
				content: '';
				display: inline-block;
				color: transparent;
				padding-right: 0px;
			}
		} /* //label */
		input {
			
		} /* //input */
		&>u, &>div>u {
			//background: rgba($action-warning, .7);
		} /* //&>u, &>div>u */
	} /* //&.control-mandatory */
	&::before {
		content: none;
		display: block;
		width: 24px;
		height: 24px;
		// background-color: $action-danger;
		position: absolute;
		left: -18px;
		/*top: 50%;*/
		/*margin-top: -11px;*/
		bottom: 20px;
		// background: url('#{$images-icon-path}login-sprite.png') no-repeat 0 0;
		@include opacity(.3);
		@include transition;
	} /* //&::before */
	// &.ic-type {margin-right: 0;}
