/* -- Promo Slider */
.login-container .adbanner-container .promo-container {
	/*position: relative;
	display: block;*/
	width: 100%;
	height: 100%;
	/*flex: 0;*/
	display: block;
	/*display: inline-block;*/
	text-align: center;
} /* //.promo-container */
.promo-container .swiper-container {
	width: 100%;
	height: 100%;
	/*display: none;*/
} /* //.swiper-container */
.promo-container .swiper-container .swiper-wrapper {
	/*display: block;*/
	/*width: 100%;*/
} /* //.swiper-wrapper */
.promo-container .swiper-container .swiper-wrapper .swiper-slide {
	/*width: 100%;*/
	overflow: hidden;
} /* //.swiper-slide */
.promo-container .swiper-container .swiper-wrapper .swiper-slide h1 {margin: 0;padding: 0;font-size: 3em;color: white;/*height: 100%;*/padding-top: 40%;text-align: right;padding-right: 50px;text-shadow: 0 4px 20px rgba(0, 0, 0, .8);}
.promo-container .swiper-container .swiper-wrapper .swiper-slide p {
	text-align: right;
	padding: 20px;
    /*padding-right: 50px;*/
    text-shadow: 0 4px 20px rgba(0, 0, 0, .8);
    color: white;
    background: rgba(0, 0, 0, .5);
    margin: 60px 60px 60px 100px;
}
.promo-container .swiper-container .swiper-wrapper .swiper-slide a {position: absolute;bottom: 10%;right: 30px;color: white;}
.promo-container .swiper-container .swiper-wrapper .swiper-slide .abanner {
	position: absolute;
	left: -25px;
	top: -20px;
	z-index: -1;
	filter: blur(5px);
	/*filter: blur(1.5rem);*/
}
.promo-container .swiper-container .swiper-pagination {
	
} /* //.swiper-pagination */
.promo-container .swiper-container .swiper-pagination.swiper-pagination-bullets {
	text-align: right;
	padding-right: 20px;
	bottom: 12px;
} /* //.swiper-pagination.swiper-pagination-bullets */
.promo-container .swiper-container .swiper-pagination.swiper-pagination-bullets span.swiper-pagination-bullet {
	width: 30px;
	height: 2px;
	border-radius: 0;
	outline: none;
	cursor: pointer;
	background: rgba(255, 255, 255, .9);
}
.promo-container .swiper-container .swiper-pagination.swiper-pagination-bullets span.swiper-pagination-bullet.swiper-pagination-bullet-active {
	background: rgba(255, 255, 255, .7);
}
.promo-container .swiper-container .swiper-button-prev, 
.promo-container .swiper-container .swiper-button-next {
	background: none;
	outline: none;
	/*position: relative;*/
} /* //.swiper-button-prev */
.promo-container .swiper-container .swiper-button-next {
} /* //.swiper-button-next */
.promo-container .swiper-container .swiper-button-prev::after, 
.promo-container .swiper-container .swiper-button-next::after {
	content: '';
	display: block;
	width: 26px;
	height: 26px;
	/*background-color: yellow;*/
	border: 3px solid white;
	border: 3px solid rgba(255, 255, 255, .8);
	border-top: 0;
	border-right: 0;
	transform: rotate(45deg);
	position: absolute;
	top: 8px;
	left: 8px;
}
.promo-container .swiper-container .swiper-button-next::after {transform: rotate(225deg);left: auto;right: 8px;}


