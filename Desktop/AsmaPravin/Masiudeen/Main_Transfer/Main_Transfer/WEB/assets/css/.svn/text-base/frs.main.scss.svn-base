/* //Bootstrap modal overriding */
/* ====== Bootstrap Overwriting :: Start */
.modal {
	.modal-dialog {
		margin: 0 auto;
		.modal-content {
			border: none;
			@include border-radius(0);
			position: relative;
			padding: 0px;
			height: 100vh;
			overflow: hidden;
			.modal-header {
				/* -- @ahmad */
				padding: 0px;
				border-bottom: 1px solid #eee;
				position: absolute;
				left: 0;
				right: 0;
				top: 0;
				z-index: 10;
				display: block;
				background-color: rgba($white, .9);
				// @include transition(all .3s ease .5s);
				// @include transform(translate(0, -100px));
				.mh-top {
					background-color: rgba($brand, .1);
					padding: 5px 0;
					text-align: center;
					h6 {
						margin: 0;
						padding: 0;
						font-size: .7rem;
					} /* //h6 */
				} /* //.mh-top */
				.mh-bottom {
					text-align: center;
					h4 {
						// color: $black;
						margin: 0;padding: 0;
						padding: 10px 0;
						font-size: 1.2rem;
					} /* //h4 */
					.mh-cross {
						width: 42px;
						height: 42px;
						display: inline-block;
						position: relative;
						// background: red;
						float: right;
						// position: absolute;
						// right: 10px;
						// top: 50%;
						// margin-top: -10px;
						@include opacity(.7);
						// @include border-radius(50px);
						&::before, &::after {
							position: absolute;
							left: 20px;
							content: ' ';
							height: 20px;
							width: 2px;
							top: 12px;
							background-color: $black;
							// @include transition(transform .4s ease, margin .2s ease);
						} /* //&::before, &::after */
						&::before {
							transform: rotate(45deg);
						} /* //&::before */
						&::after {
							transform: rotate(-45deg);
						} /* //&::after */
						&:hover {
							background-color: rgba($action-danger, 1);
							&::before, &::after {background-color: $white;}
						} /* //&:hover */
					} /* //.mh-cross */
				} /* //.mh-bottom */
			} /* //.modal-header */
			.modal-body {
				position: relative;
				height: 100%;
				padding: 60px 15px 0;
				@include transition(all .3s ease .5s);
				// @include transition;
				.modal-overflow {
					overflow: auto;height: 100%;
				} /* //.modal-overflow */
			} /* //.modal-body */
			.modal-footer {
				border-top: 1px solid #eee;
				text-align: center;
				/* -- @ahmad */
				position: absolute;
				left: 0;right: 0;bottom: 0;
				z-index: 10;
				background-color: rgba($white, .9);
				// display: block;
				justify-content: space-between;
				@include transition(all .3s ease .5s);
				@include transform(translate(0, 100px));
				// .modal-open .modal .modal-content & {@include transform(translate(0, 0));}
			}/*.modal-footer*/
		} /* //.modal-content */
		&.modal-custom {
			max-width: 100%;
		} /* //&.modal-custom */
	} /* //.modal-dialog */
} /* //.modal */
// .modal-open .modal.show .modal-dialog .modal-content .modal-header {@include transform(translate(0, 0));}
// .modal-open .modal.show .modal-dialog .modal-content .modal-body {padding-top: 75px;padding-bottom: 85px;}
.modal-open .modal.show .modal-dialog .modal-content .modal-body {padding-bottom: 85px;}
.modal-open .modal.show .modal-dialog .modal-content .modal-footer {@include transform(translate(0, 0));}

