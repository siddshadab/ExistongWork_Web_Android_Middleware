/* ====== Search Box :: Start */
.help-box {
	/*position: fixed;
	left: 0;top: 0;right: 0;bottom: 0;
	background-color: rgba($black, .9);
	z-index: 152;
	overflow: auto;*/
	// position: fixed;
	// left: 0;
	// top: 0;
	// right: 0;
	// bottom: 0;
	// background-color: rgba($black, .6);
	// height: 0;
	// z-index: 152;
	// z-index: 220;
	// padding-right: 17px;
	// @include transform(tranlate(-100%, 0));
	// overflow: hidden;
	// @include transition(background .35s linear, height .35s ease-in 200ms);
	padding-right: 17px;display: block;
	display: none;
	.help-close {
		// background-color: rgba($white, .2);
		background-color: rgba($white, .1);
		display: block;
		width: 36px;
		height: 36px;
		position: fixed;
		// right: -40px;top: -40px;
		right: 10px;top: 10px;
		z-index: 820;
		@include border-radius(100%);
		@include transform(scale(0));
		// @include transition;
		@include transition(all .2s ease 0s);
		&::before, &::after {
			content: '';
			display: block;
			position: absolute;
			width: 16px;
			height: 2px;
			@include transform(rotate(45deg));
			@include border-radius(1px);
			@include transition(all .3s cubic-bezier(0.21, 0.98, 0.6, 0.99));
			// background-color: $white;
			background-color: rgba($white, .8);
			left: 18px;top: 18px;
			margin-left: -8px;
			// margin-top: -9px;
		} /* //&::before, &::after */
		&::after {@include transform(rotate(-45deg));} /* &::after */
		&:hover {
			// @include transform(scale(1.2));
			// background-color: rgba($white, .5);
			background-color: rgba($white, .3);
			&::before, &::after {background-color: rgba($white, .96);}
		} /* //&:hover */
		&:active {
			@include transform(scale(0.94));
		} /* //&:active */
	} /* //.help-close */
	.help-overlay {
		position: fixed;
		left: 0;
		top: 0;
		right: 0;
		bottom: 0;
		background-color: rgba($black, .6);
		z-index: 220;
		// display: none;
		@include opacity(0);
		@include transition(opacity 0.6s ease);
	} /* //.help-overlay */
	.inhelp-container {
		width: 320px;
		// background-color: rgba($white, .9);
		// background-color: rgba($white, .98);
		background-color: rgba($brand, .98);
		position: fixed;
		right: 0;
		z-index: 230;
		top: 0;bottom: 0;
		@include transform(translate(100%, 0));
		// @include transition(all .4s cubic-bezier(0.4, 0.0, 0.2, 1));
		@include opacity(0);
		@include transition(all .4s cubic-bezier(0.21, 0.98, 0.6, 0.99));
		// @include transition(all .4s cubic-bezier(0.21, 0.98, 0.6, 0.99));
		flex: 1 1 100%;
		display: flex;
		flex-direction: column;
		.hb-header {
			// display: none;
			// background-color: purple;
			h4 {
				text-align: center;
				margin: 0;
				padding: 20px 0 10px;
				font-size: 1.1rem;
				color: rgba($white, .8);
			} /* //h4 */
		} /* //.hb-header */
		.hb-content {
			// background-color: red;
			margin: auto;
			overflow: auto;
			// height: 100%;
			height: auto;
			.help-search {
				padding: 10px 0 10px;
				.hsearch-form {
					.hsearch-control {
						// border-bottom: 2px solid $brand;
						border-bottom: 2px solid rgba($white, .2);
						position: relative;
						padding: 5px 10px 5px 40px;
						width: 80%;
						margin: 0 auto;
						@include transition;
						&::after {
							content: '';
							position: absolute;
							height: 2px;
							display: block;
							bottom: -2px;
							background-color: $white;
						} /* //&::after */
						&>span {
							@include opacity(.6);
							position: absolute;
							width: 24px;
							height: 24px;
							// background: red;
							left: 5px;
							top: 8px;
							background: url('#{$images-icon-path}ic-search-w.png') no-repeat;
							background-size: 22px;
						} /* //&>span */
						input[type="search"] {
							@include appearance();
							background: transparent;
							color: $white;
							border: none;
							font-size: 20px;
							width: 100%;
							&:focus {
								outline: none;
							} /* //&:focus */
							@extend .placeholder-s-w;
						} /* //input[type="search"] */
						&.hsc-active {
							border-bottom-color: $white;
						} /* //&.hsc-active */
					} /* //.hsearch-control */
				} /* //.hsearch-form */
			} /* //.help-search */
			.help-list {
				padding: 0 15px;
				.dl-help-items {
					font-size: .9rem;
					color: rgba($white, .7);
					// counter-reset: counter-help-list;
					&>div {
						padding-left: 22px;
						display: block;
						padding: .5rem 0;
						padding-left: 30px;
						dt {
							position: relative;
							&::before {
								// content: "";
								content: "\2022";
								// counter-increment: counter-help-list;
								font-size: .8rem;line-height: 1.2;
								position: absolute;
								left: -22px;top: 5px;
								display: block;
								width: 12px;height: 12px;
								@include opacity(.9);
								font-size: 30px;
								font-weight: 600;
								line-height: 10px;
								// color: $action-warning;
							} /* //&::before */
							&.ex-link {
								&::before {
									content: "";
									background: url('#{$images-icon-path}external-w.png') no-repeat right center;
									background-size: 12px;
									@include opacity(.7);
								} /* //&::before */
							} /* //&.ex-link */
							a {
								color: rgba($white, .8);
								display: inline-block;
								@include transition;
								&:hover {
									// color: rgba($white, 1);
								} /* //&:hover */
							} /* //a */
						} /* //dt */
						dd {
							font-size: .7rem;
							margin-bottom: 0;
							@include opacity(.6);
							@include transition;
