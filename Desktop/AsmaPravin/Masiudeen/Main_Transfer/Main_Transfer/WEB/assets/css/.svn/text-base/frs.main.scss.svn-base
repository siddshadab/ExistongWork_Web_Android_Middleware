/* ----------------------------
	Tiny Modals :: Start
--------------------------- */
.tiny-modal {
	display: none;
	.tinymodal-overlay {
		position: fixed;
		left: 0;top: 0;right: 0;bottom: 0;
		/*background: rgba($black, .86);*/
		background: rgba($black, .6);
		// z-index: 200;
		z-index: 1060;
		visibility: hidden;
		@include opacity(0);
		@include transition2(opacity 0.4s, visibility .01s);
	} /* //.tinymodal-overlay */
	.tinymodal {
		position: fixed;
		background: rgba($white, .96);
		width: 300px;
		/*width: 320px;*/
		/*height: 200px;*/
		top: 30%;
		left: 50%;
		margin-left: -150px;
		// z-index: 202;
		z-index: 1065;
		@include backface-visibility;
		/*@include box-shadow;*/
		overflow: hidden;
		@include box-shadow(0 25px 50px rgba(0,0,0,0.5));

		visibility: hidden;
		@include opacity(0);
		@include transform(scale(0.7));
		@include transition2(opacity 0.4s, transform 0.4s, visibility .4s ease .01s);
		
		.tinymodal-header {
		} /* //.tinymodal-header */
		.tinymodal-content {
			padding: 15px 15px 25px;
			.tinymodal-icon {
				/*padding-left: 10px;*/
				padding-top: 10px;
				position: relative;
				width: 32px;
				height: 32px;
				&::before {
					content: '';
					display: block;
					width: 28px;
					height: 28px;
					position: absolute;
					background: url('#{$images-icon-path}response-icons.png') no-repeat 0 0;
					left: 2px;top: 2px;
					@include opacity(.7);
					// @include transform(scale(.9));
				} /* //&::before */
				&.error {
					&::before {
						background-position: 0 -28*2px;
						@include opacity(1);
					}
				} /* //&.error */
				&.success {
					&::before {
						background-position: 0 -28*4px;
						@include opacity(1);
					}
				} /* //&.success */
				&.warning {
					&::before {
						background-position: 0 -28*6px;
						@include opacity(1);
					}
				} /* //&.warning */
			} /* //.tinymodal-icon */

			.tinymodal-info {
				/*padding-right: 20px;*/
				h4 {
					margin: 0;padding: 0 0 5px;
					// font-size: 18px;
					font-weight: normal;
					color: $black;
					color: rgba($black, .8);
					font-weight: 500;
					font-size: 1.2rem;
				} /* //h4 */
				p {
					margin: 0;padding: 0;
					color: $black;
					color: rgba($black, .6);
					font-size: .8rem;
				} /* //p */
			} /* //.tinymodal-info */

		} /* //.tinymodal-content */
		.tinymodal-footer {
		} /* //.tinymodal-footer */

		.tm-button {
			padding: 10px 10px;
			text-align: center;
			display: block;
			text-transform: uppercase;
			font-size: 15px;
			cursor: pointer;
			&.default {
				font-weight: normal;
				background-color: rgba($black, .1);
			} /* //.default */
			&.primary {
				font-weight: normal;
				background-color: rgba($brand, 1);
				color: $white;
			} /* //.primary */
			&.error {
				font-weight: normal;
				background-color: rgba($action-danger, .9);
				color: $white;
			} /* //.error */
			&.success {
				font-weight: normal;
				background-color: rgba($action-success, .9);
				color: $white;
			} /* //.success */
			&:hover {
				text-decoration: none;
			} /* //&:hover */
		} /* //.tm-button */

	} /* //.tinymodal */

	&.tinymodal-showing {
		/*@include transition(opacity 0.4s, transform 0.4s, visibility .4s);*/
		.tinymodal-overlay {
			visibility: visible;
			@include opacity(1);
		} /* //.tinymodal-overlay */
		.tinymodal {
			visibility: visible;
			@include transform(scale(1));
			@include opacity(1);
		} /* //.tinymodal */
	} /* //&.tinymodal-showing */

	&.tm-rounded .tinymodal {
		@include border-radius(8px);
		.tm-button.tm-default {@include border-radius(0 0 0 8px);}
		.tm-button.tm-primary, .tm-button.tm-danger {@include border-radius(0 0 8px 0);}

		.tinymodal-footer.tmf-fullbuttons {
			.tm-button.tm-primary, .tm-button.tm-danger {@include border-radius(0);}
		} /* //&.tmf-fullbuttons */
	} /* //.tm-rounded */
	.tinymodal-footer.tmf-fullbuttons {
		.tm-button.tm-primary, .tm-button.tm-danger {@include border-radius(0);}
	} /* //&.tmf-fullbuttons */

} /* //.tiny-modal */
/* ----------------------------
	Tiny Modals :: End
--------------------------- */

