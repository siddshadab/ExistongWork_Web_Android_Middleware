/*====================  COMPONENT STYLE COMPONENT STYLE STARTS  =====================================*/

.page-loader {
	position: fixed;
	overflow: hidden;
	left: 0;right: 0;top: 0;bottom: 0;
	background: rgba($black, .8);
	display: none;
	z-index: 600;
	visibility: hidden;
	@include opacity(0);
	// @include transition(all);
	@include transition2(opacity 0.6s, visibility .01s);
	.loader {
		position: absolute;
		width: 120px;
		height: 140px;
		// background: $white;
		display: block;
		left: 50%;
		margin-left: -60px;
		// top: 100px;
		top: 50%;
		margin-top: -70px;
		text-align: center;
		&>div {
			width: 80px;
			height: 80px;
			// border: 2px solid red;
			margin: 10px auto 5px;
			position: relative;
			&::before, &::after {
				content: '';
				display: block;
				position: absolute;
				width: 100%;
				height: 100%;
				@include border-radius(100%);
				border: 3px solid $brand;
				// @include box-shadow(0 2px 12px rgba($white, .7));
				@include animation(pulse-loader 1.2s ease-out infinite);
			} /* //&::before, &::after */
			&::after {
				// @include transform(scale(.8));
				@include animation-delay(.38s);
			}
		} /* //&>div */
		em {
			display: block;
			font-style: normal;
			font-weight: 500;
			color: rgba($white, .7);
			padding: 10px 0;
			letter-spacing: 2px;
		} /* //em */
	} /* //.loader */
	&.inprogress {
		display: block;
		visibility: visible;
		@include opacity(1);
	} /* //&.inprogress */
} /* //.page-loader */

