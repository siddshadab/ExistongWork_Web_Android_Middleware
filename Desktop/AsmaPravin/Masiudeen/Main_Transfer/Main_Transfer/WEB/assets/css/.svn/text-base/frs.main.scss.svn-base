						width: 32px;
						height: 3px;
						background: $brand;
						position: absolute;
						right: 0px;top: 15px;
						@include transition2(background .2s ease);
							&::before, &::after {
								content: '';
								position: absolute;
								width: 100%;
								height: 3px;
								margin-top: -7px;
								background: $brand;
								@include transition(transform .4s ease, margin .2s ease);
							} /* //&::before, &::after */
							&::after {
								margin-top: 7px;
							} /* //&::after */
						} /* //&>div */
					} /* //&>div */
					span {
						display: inline-block;
						vertical-align: middle;
					} /* //span */
					&.menu-active {
						background: rgba($brand , .2);
						// @include border-radius(4px);
						&>div {
							&>div {
								background: transparent;
								&::before, &::after {
									margin-top: 0;
								} /* //&::before, &::after */
								&::before {
									@include transform(rotate(45deg) scale(.76, .96));
								} /* //&::before */
								&::after {
									@include transform(rotate(-45deg) scale(.76, .96));
								} /* //&::after */
							} /* //&>div */
						} /* //&>div */
					} /* //&.menu-active */
				} /* //&.item-nav */
				&::before {
					content: '';
					display: block;
					opacity: 0;
					position: absolute;
					@include transition-duration(.15s);
					@include transition-timing-function(cubic-bezier(0.4,0.0,0.2,1));
					z-index: -1;
					bottom: 0;
					left: 0;
					right: 0;
					top: 0;
					background: rgba($brand, .5);
					// @include border-radius(4px);
					// @include border-radius(30px);
					@include transform(scale(0));
					@include transition-property(transform, opacity);
				} /* //&::before */
				&:hover {
					// background: rgba($brand, .1);
					text-decoration: none;
					&::before {
						// opacity: .12;
						@include transform(scale(1));
