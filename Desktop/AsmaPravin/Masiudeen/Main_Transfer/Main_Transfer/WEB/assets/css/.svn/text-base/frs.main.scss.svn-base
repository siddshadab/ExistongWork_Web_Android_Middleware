/* -- Tabs */
nav.tab-controller {
	.tabs {
		padding: .6rem 0;
		width: 100%;
		overflow: hidden;
		overflow-x: auto;
		text-align: center;
		h5 {
			font-size: .76rem;
			display: inline-block;
			vertical-align: middle;
			line-height: inherit;
			color: rgba($black, .7);
			margin: 0 1rem 0 0;padding: 3px 0 0;
		} /* //h5 */
		.lst-tabs {
			list-style: none;
			margin: 0;padding: 0;
			position: relative;
			display: inline-block;
			vertical-align: middle;
			white-space: nowrap;
			li {
				display: inline-block;
				margin: 0;padding: 0 .3rem;
				a {
					cursor: pointer;
					text-decoration: none;
					// padding: .5rem 1.2rem;
					padding: .6rem 1.5rem .45rem;
					position: relative;
					line-height: inherit;
					@include border-radius(30px);
					background: rgba($brand, .06);
					display: block;
					@include transition;
					border: 1px solid rgba($brand, .1);
					font-size: .9rem;
					// z-index: 0;
					&::before {
						content: '';
						display: block;
						@include opacity(0);
						position: absolute;
						@include transition-duration(.15s);
						@include transition-timing-function(cubic-bezier(0.4,0.0,0.2,1));
						z-index: -1;
						bottom: 0;
						left: 0;
						right: 0;
						top: 0;
						background: rgba($brand, .5);
						// @include border-radius(4px);
						@include border-radius(30px);
						@include transform(scale(0));
						@include transition-property(transform, opacity);
					} /* //&::before */
					&:hover {
						text-decoration: none;
						&::before {
							// opacity: .12;
							@include transform(scale(1));
							@include opacity(.3);
						} /* //&::before */
					} /* //&:hover */
					&.tab-selected {
						color: $white;
						cursor: default;
						background: rgba($brand, .9);
						// @include animation(grow .3s cubic-bezier(0.4,0.0,0.2,1));
						&::after {
							// content: '';
							display: block;
							width: 10px;
							height: 10px;
							background: red;
						} /* //&::after */
					} /* //&.tab-selected */
				} /* //a */
				&.tab-title {
					font-size: .7rem;
					line-height: inherit;
					vertical-align: middle;
				} /* //&.tab-title */
				&:first-child {
					
				} /* //&:first-child */
				&:last-child {
					
				} /* //&:last-child */
			} /* //li */
		} /* //.lst-tabs */
	} /* //.tabs */
	.tabs-container {
			border: 1px solid #eee;
		    background: #f9f9f9;
		.tab-content {
			display: none;
			&.tc-showing {
				display: block;
			} /* //&.tc-showing */
		} /* //.tab-content */
	} /* //.tabs-container */
} /* //nav.tab-controller */

/* -- checkbox and radio */
label.selection {
	display: inline-block;
	vertical-align: middle;
	position: relative;
	padding-left: 28px;
	padding-right: 20px;
	cursor: pointer;
	// height: 30px;
	margin-bottom: 0;
	font-size: 1rem;
	em {
		// all: unset;
		font-style: normal;
		@include opacity(.7);
		font-weight: 500;
		// padding-top: 1px;
		display: inline-block;
		vertical-align: middle;
		text-transform: none;
		color: $black;
	} /* //em */
	input[type="radio"],
	input[type="checkbox"] {
		position: absolute;
		left: 4px;
		top: 50%;
		width: 17px;
		height: 17px;
		border: 2px solid rgba($black, .3);
		display: block;
		margin-top: -10px;
		cursor: pointer;
		z-index: 0;
		@include appearance(none);
		// &::-ms-check {
		// 	display: none;
		// }
		&:focus {
			outline: none;
		} /* //&:focus */
		&::before {
			height: 50px;
			width: 50px;
			margin-left: -16px;
			margin-top: -16px;
			@include opacity(0);
			content: '';
			display: block;
			position: relative;
			z-index: 10;
			@include border-radius(100%);
			background: rgba($brand, .7);
		} /* //&::before */
	}
	input[type="radio"] {
		width: 20px;
		height: 20px;
		@include border-radius(100%);
		&:checked {
			border-color: rgba($brand, 1);
			border-width: 2px;
			&::before {
				@include animation(ripple .3s cubic-bezier(0.4,0.0,0.2,1));
			} /* //&::before */
			&::after {
				content: '';
				position: absolute;
				width: 10px;
				height: 10px;
				left: 50%;
				top: 50%;
				background-color: rgba($brand, 1);
				margin-top: -5px;
				margin-left: -5px;
				@include border-radius(100%);
			} /* //&::after */
			& ~ em {
				// color: $brand;
				@include opacity(1);
			}
			&:disabled {
				border-color: rgba($black, .3);
				// background: rgba($black, .1);
				&::after {
					background-color: rgba($black, .3);
				}
			} /* //&:disabled */
		} /* //&:checked */
	} /* //input[type="radio"] */
	input[type="checkbox"] {
		@include border-radius(4px);
		// @include border-radius(100%);
		&:checked {
			border-color: rgba($brand, 1);
			border-width: 2px;
			background: rgba($brand, 1);
			&::before {
				@include animation(ripple .3s cubic-bezier(0.4,0.0,0.2,1));
			} /* //&::before */
			&::after {
				content: '';
				position: absolute;
				width: 12px;
				height: 6px;
				border: 2px solid $white;
				border-top: 0px solid transparent;
				border-right: 0px solid transparent;
				left: 50%;
				top: 50%;
				margin-left: -6px;
				margin-top: -4px;
				@include transform(rotate(-45deg));
			} /* //&::after */
			& ~ em {
				// color: $brand;
				@include opacity(1);
			}
			&:disabled {
				border-color: rgba($black, .3);
				background: rgba($black, .1);
				&::after {
					border-color: rgba($black, .3);
				}
			} /* //&:disabled */
		} /* //&:checked */
	} /* //input[type="checkbox"] */
	&:hover {
		input[type="radio"] {
			background: rgba($brand, .1);
			&:checked {
				// background: rgba($brand, 1);
			} /* //&:checked */
		}
		input[type="checkbox"]  {
			background: rgba($brand, .1);
			&:checked {
				background: rgba($brand, 1);
				&:disabled {
					border-color: rgba($black, .3);
					background: rgba($black, .1);
					&::after {
						border-color: rgba($black, .3);
					}
				} /* //&:disabled */
			} /* //&:checked */
		}
	} /* //&:hover */
	input[type="radio"]:disabled, 
	input[type="checkbox"]:disabled {
		background: rgba($black, .1);
		cursor: not-allowed;
		&::before {content: none;}
	}
	input[type="radio"]:disabled ~ em, 
	input[type="checkbox"]:disabled ~ em {
		cursor: not-allowed;
		@include opacity(.5);
	}
} /* //label.selection */


/* -- Toggle Switch */
label.toggle {
	position: relative;
	cursor: pointer;
	display: inline-block;
	margin-bottom: 0;
	line-height: 0;
	@include border-radius(20px);
	input[type="checkbox"] {
		cursor: pointer;
		border: 2px solid rgba($black, .2);
		background-color: $white;
		@include border-radius(20px);
		width: 44px;
		height: 22px;
		@include appearance(none);
		// @include transition;
		&::before {
			content: '';
			width: 18px;
			height: 18px;
			// background: rgba($black, .3);
			@include box-shadow(0 0 10px rgba($black, .6));
			background: rgba($white, .9);
			@include border-radius(100%);
			position: absolute;
			// left: 5px;top: 5px;
			left: 2px;top: 2px;
			@include transition(all .4s cubic-bezier(0.4,0.0,0.2,1));
			// @include transform(scale(1.1));
		} /* //&::before */
		&:checked {
			border-color: $brand;
			background-color: $brand;
			&::before {
				background: $white;
				@include transform(translate(22px, 0));
			}
			&:disabled {
				border-color: rgba($black, .2);
				background-color: rgba($black, .3);
				&::before {
					background: rgba($white, .7);
				}
			} /* //&:disabled */
		} /* //&:checked */
		&:disabled {
			cursor: not-allowed;
			background-color: rgba($black, .1);
			&::before {
				background: rgba($white, .1);
			}
		} /* //&:disabled */
		&:focus {
			outline: none;
		} /* //&:focus */
	} /* //input[type="checkbox"] */
	&.sm {

	} /* //&.sm */
} /* //label.toggle */

/* -- Tooltip overtip */
.overtip {
	position: relative;
	text-align: center;
	&::before {
		content: attr(overtip-content);
		position: absolute;
		left: 50%;
		top: -40px;
		display: inline-block;
		width: 100px;
		margin-left: -50px;
		background-color: rgba($black, 0.8);
		font-size: .7rem;
		color: white;
		z-index: 2;
		// z-index: 1050;
		text-indent: 0;
		text-align: center;
		font-style: normal;
		padding: .3rem .5rem;
		@include border-radius;
		@include opacity(0);
		visibility: hidden;
		// z-index: -1;
		@include transition-duration(.3s);
		@include transition-timing-function(cubic-bezier(0.4,0.0,0.2,1));
		@include transition-property(visibility, opacity);
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
	} /* //&::before */
	&::after {
		content: '';
		position: absolute;
		width: 0px;
		height: 0px;
		/*border: 10px solid red;*/
		border: 10px solid transparent;
		border-top-color: rgba($black, 0.8);
		border-bottom: 0 solid transparent;
		left: 50%;
		top: -15.5px;
		margin-left: -10px;
		@include opacity(0);
		visibility: hidden;
		// z-index: -1;
		@include transition-duration(.3s);
		@include transition-timing-function(ease);
		@include transition-property(visibility, opacity);
	} /* //&::after */
	&:hover {
		&::before, &::after {
			@include opacity(1);
			visibility: visible;
			// z-index: 0;
		}
	} /* //&:hover */
} /* //.overtip */

/* -- badge */
.badge {
	font-weight: normal;
	font-size: .7rem;
	padding: .2rem .5rem;
	// vertical-align: middle;
	@include border-radius(10px);
	&.primary {
		color: $white;
		background: $brand;
	} /* //&.primary */
	&.secondary {
		// color: $white;
		background: rgba($black, .2);
	} /* //&.secondary */
	&.success {
		color: $white;
		background: rgba($action-success, .7);
	} /* //&.success */
	&.danger {
		color: $white;
		background: rgba($action-danger, .7);
	} /* //&.danger */
	&.warning {
		background: rgba($action-warning, .5);
	} /* //&.warning */
} /* //.badge */


/* -- toast message */
.msg-toast {
	position: fixed;
	background: rgba($black, .9);
	color: $white;
	top: 30px;
	left: 50%;
	// width: 300px;
	// margin-left: -150px;
	// z-index: 120;
	z-index: 1060;
	text-align: center;
	@include box-shadow(0 0 20px rgba($black, .3));
	@include opacity(0);
	@include transform(translate(-50%, -20%));
	@include transition;
	width: 300px;
	margin-left: 0;
	max-width: 300px;
	// transform: translateX(-50%);
	@include transform(translateX(-50%));
	left: 50%;
	em {
		padding: .8rem 1rem;
		display: block;
		font-weight: normal;
		font-style: normal;
		font-size: .8rem;
	} /* //em */
	&.msg-success {
		background: rgba($action-success, .8);
	} /* //&.msg-success */
	&.msg-error {
		background: rgba($action-danger, .8);
	} /* //&.msg-error */
	&.msg-warning {
		background: rgba($action-warning, .8);
	} /* //&.msg-warning */
	&.msg-close {
		padding-right: 30px;
		cursor: pointer;
		.tm-cross {
			width: 20px;
			height: 20px;
			position: absolute;
			right: 10px;
			top: 50%;
			margin-top: -10px;
			@include opacity(.7);
			&::before, &::after {
				position: absolute;
				left: 10px;
				content: ' ';
				height: 100%;
				width: 2px;
				background-color: $white;
				// @include transition(transform .4s ease, margin .2s ease);
			} /* //&::before, &::after */
			&::before {
				transform: rotate(45deg);
			} /* //&::before */
			&::after {
				transform: rotate(-45deg);
			} /* //&::after */
		} /* //.tm-cross */
	} /* //&.msg-close */
	&.msg-showing {
		@include transform(translate(-50%, 0));
		@include opacity(1);
		// @include transition;
	} /* //&.msg-showing */
} /* //.msg-toast */

/* --Datepicker--*/

.ui-datepicker{
	background:$white;
	padding:0px !important;
	.ui-datepicker-header{
		background:$brand;
		color: white;
		.ui-datepicker-title{
			font-weight: 500;
		}/* //.ui-datepicker-title */
		&.ui-widget-header{
			.ui-icon.ui-icon-circle-triangle-e{
				content: "";
			    width: 8px;
			    height: 8px;
			    border: 2px solid $white;
			    border-top: 0;
			    border-right: 0;
			    display: block;
			    position: absolute;
			    right: 5px;
			    top: 50%;
			    margin-left: -6px;
			    margin-top: -4px;
			    transform: rotate(-135deg);
			}
			.ui-icon.ui-icon-circle-triangle-w{
				content: "";
			    width: 8px;
			    height: 8px;
			    border: 2px solid white;
			    border-top: 0;
			    border-right: 0;
			    display: block;
			    position: absolute;
			    right: 5px;
			    top: 50%;
			    margin-left: -2px;
			    margin-top: -4px;
			    transform: rotate(45deg);
			}
			.ui-datepicker-prev-hover{
				top: 2px;
				left: 2px
			}
			.ui-datepicker-next-hover{
				top: 2px;
				right: 2px;
			}
			.ui-state-hover{
				border: 1px solid #ccc;
				background: transparent;
				font-weight: normal;
				color: #2b2b2b;
			}
		}
	}/* //.ui-datepicker-header */	
	.ui-datepicker-calendar{
		th{
			padding: .7em .3em;
		    text-align: center;
		    font-weight: 600;
		    border: 0;
		    opacity: .7;
		    &.ui-datepicker-week-end{
				color:#000;
				font-weight: 500;
				opacity: 0.7;
			}
		}
		.ui-state-default{
			border: 1px solid transparent;
		    background:transparent;
		    text-align: center;
		    font-weight: 600;
		}
		.ui-state-active{
			background: #007fff;
		    font-weight: normal;
		    color: #fff;
		}
		.ui-state-hover{
			border: 1px solid #ccc;
			background: #ededed;
			font-weight: normal;
			color: #2b2b2b;
		}
	}/* //.ui-datepicker-calendar*/	
	.ui-datepicker-week-end{
		color:#000;
		font-weight: 500;
		opacity: 0.7;
		a{
			&.ui-state-default{
				color:#000;
				font-weight:500;
				opacity: 0.7;
				&.ui-state-active{
					color: $white;
					&.ui-state-hover{
						color:black;
					}
				}
			}
		}
	}
	.ui-datepicker-today{
		.ui-state-highlight{
			border: 1px solid rgba(255, 0, 0, 0.6);
			color: black;
			&.ui-state-active{
				border: 1px solid #ccc;
				color:white;
				&.ui-state-hover{
					color: black;
				}
			}
		}
		.ui-state-hover{
			border: 1px solid rgba(255, 0, 0, 0.6);
			color: black;
		}
	}
}/* //.ui-datepicker */

