								} /* //&::before */
								span.chevron {
									&::after{border-color: rgba($white, .9);}
								}
							} /* //&:hover */
							&.last-active {
								background-color: $second-brand;
								text-decoration: none;
								color: $white;
								&::before {
									// opacity: .12;
									@include transform(scale(1));
									@include opacity(1);
								} /* //&::before */
								span.chevron {
									&::after{border-color: rgba($white, .9);}
								}
							}
							&::after {
								content: none;
								display: inline-block;
								width: 6px;
								height: 6px;
								background: $brand;
								left: 50%;
								margin-left: -3px;
								border-radius: 3px;
								bottom: 5px;
								position: absolute;
							} /* //&::after */
							span.chevron {
								width: 12px;
								height: 12px;
								display: block;
								position: absolute;
								right: 5px;top: 50%;
								margin-top: -6px;
								@include transition;
								&::after {
									content: '';
									width: 8px;
									height: 8px;
									// border: 2px solid rgba($black, .5);
									border: 2px solid rgba($white, .8);
									border-top: 0;border-right: 0;
									margin-left: 2px;
									display: block;
									@include transform(rotate(-45deg));
								} /* //&::after */
							} /* //span.chevron */
							&.glnavi-active {
								// background-color: rgba($brand , 1);
								background-color: darken($brand , 5%);
								color: $white;
								&::after {background: $white;}
								span.chevron {
									@include transform(rotate(180deg));
									&::after{border-color: rgba($white, .9);}
								} /* //&::after */
							} /* //.glnavi-active */
							&.glnavi-selected {
								// color: rgba($brand, .9);
								// color: rgba($white, .9);
								&::after {content: none;}
							} /* //&.glnavi-selected */
							&:hover {
								// background-color: #aaa;
								text-decoration: none;
							} /* //&:hover */
						} /* //a */
						.subnavigation-items {
							// display: none;
						} /* //.subnavigation-items */
						&.mb-help {
							
						} /* //&.mb-help */
						&.mb-template {
							
						} /* //&.mb-template */
						&.mb-logout {
							a {
								color: lighten($action-danger, 30%);
								// background: rgba($action-danger, .1);
							}
						} /* //&.mb-logout */
					} /* //li */
				} /* //.lst-navigation */
			} /* //.navigation-items */
		} /* //.glnav-content */
	} /* //div.in-glnav */
	&.sticky-nav {
		position: fixed;
		left: 0;right: 0;top: 61px;;
		// bottom: 0;
		z-index: 150;
		// @include transition;
		// background: $brand;
		// background: rgba($brand, .96);
		// height: 0;
