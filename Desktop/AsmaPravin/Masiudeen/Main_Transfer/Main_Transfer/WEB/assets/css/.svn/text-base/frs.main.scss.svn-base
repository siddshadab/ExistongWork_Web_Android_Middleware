.loader {
	position: absolute;
	left: 50%;top: 50%;
	// width: 100px;
	// height: 90px;
	text-align: center;
	margin-left: -50px;margin-top: -45px;
	.load-spinner {
		width: 80px;
		height: 40px;
		margin: 0 auto 10px;
		position: relative;
		@include transform(scale(.8));
		// background-color: red;
		&>div {
			width: 80px;
			height: 45px;
			position: absolute;
			left: 0;top: 0;
			// background-color: purple;
			&::after {
				content: '';
				display: block;
				margin: 0 auto;
				// inline-size: 3.5px;
				// block-size: 10.5px;
				@include border-radius;
				background-color: $white;
				@include animation(spinner .6s infinite ease-in-out both);
				width: 4px;
				height: 12px;
				// animation: c .6s infinite ease-in-out both;
			} /* //&::after */
			&:nth-child(1) 	{@include transform(rotate(30*0deg));&::after {}}
			&:nth-child(2) 	{@include transform(rotate(30*1deg));&::after {@include animation-delay(-.55s);}}
			&:nth-child(3) 	{@include transform(rotate(30*2deg));&::after {@include animation-delay(-.5s);}}
			&:nth-child(4) 	{@include transform(rotate(30*3deg));&::after {@include animation-delay(-.45s);}}
			&:nth-child(5) 	{@include transform(rotate(30*4deg));&::after {@include animation-delay(-.4s);}}
			&:nth-child(6) 	{@include transform(rotate(30*5deg));&::after {@include animation-delay(-.35s);}}
			&:nth-child(7) 	{@include transform(rotate(30*6deg));&::after {@include animation-delay(-.3s);}}
			&:nth-child(8) 	{@include transform(rotate(30*7deg));&::after {@include animation-delay(-.25s);}}
			&:nth-child(9) 	{@include transform(rotate(30*8deg));&::after {@include animation-delay(-.2s);}}
			&:nth-child(10) {@include transform(rotate(30*9deg));&::after {@include animation-delay(-.15s);}}
			&:nth-child(11) {@include transform(rotate(30*10deg));&::after {@include animation-delay(-.1s);}}
			&:nth-child(12) {@include transform(rotate(30*11deg));&::after {@include animation-delay(-.05s);}}
		} /* //&>div */
	} /* //.load-spinner */
	em {
		display: block;
		font-style: normal;
		font-weight: 500;
		color: rgba($white, .7);
		padding: 0;
		letter-spacing: 1px;
		font-size: .8rem;
	} /* //em */
} /* //.loader */
/* ====== Filter Widget Box :: End   */


