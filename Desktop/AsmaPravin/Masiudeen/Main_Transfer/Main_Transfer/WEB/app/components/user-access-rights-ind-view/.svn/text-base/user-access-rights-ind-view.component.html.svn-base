<div class="breadcrumb">
    <ul class="lst-breadcrumb">
        <!-- <li>
            <a href="javascript:;">Master/a>
        </li> -->
        <li>
            <a href="javascript:;" routerLink="/user-access-rights-view">{{ 'HOME.USER_ACCESS_RIGHTS' | translate }}</a>
        </li>
        <li class="current" *ngIf="!editActivated">{{ 'ACTION.VIEW' | translate }}</li>
        <li class="current" *ngIf="editActivated">{{ 'ACTION.EDIT' | translate }}</li>
    </ul><!--.lst-breadcrumb-->
</div>
<div class="vspacer20"></div>
<main class="main-container">
    <div class="in-main">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="in-mcontainer">
                        <div class="category">
                            <div class="row">
                                <div class="col-6">{{ 'HOME.USER_ACCESS_RIGHTS_DETAILS' | translate }}</div>
                                <div class="col-6 text-right">
                                    <div class="action-img">
                                        <img *ngIf="!editActivated && rightsData.authoritye == 1" (click)="edit();" src="../../../assets/images/icon/Edit2x.png" />
                                    </div>
                                    <div class="action-img">
                                        <img *ngIf="!editActivated && rightsData.delete == 1" (click)="delete();" src="../../../assets/images/icon/Delete2x.png" />
                                    </div>
                                </div>
                            </div>   
                        </div>
                        <div class="vspacer20"></div>
                        <form [formGroup]="userAccessRightsForm" >
                            <div class="row">
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.MENU_ID' | translate }}</label>
                                        <h6 *ngIf="!editActivated">{{userRightsData.userAccressRightsCompositeEntity.menuid | Custom}}</h6>
                                        <input *ngIf="editActivated" type="text" formControlName="menuid" placeholder="{{ 'HOME.MENU_ID' | translate }}"><u></u>
                                        <em *ngIf="userAccessRightsForm.controls['menuid'].hasError('required') && (userAccessRightsForm.controls['menuid'].dirty || userAccessRightsForm.controls['menuid'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                        <em *ngIf="userAccessRightsForm.controls['menuid'].hasError('pattern') && (userAccessRightsForm.controls['menuid'].dirty || userAccessRightsForm.controls['menuid'].touched)">{{ 'VALIDATION.NUMBERS' | translate }}</em>
                                        <em *ngIf="userAccessRightsForm.controls['menuid'].hasError('maxlength') && (userAccessRightsForm.controls['menuid'].dirty || userAccessRightsForm.controls['menuid'].touched)">{{ 'VALIDATION.MAX_8' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div><!--.col-12 col-md-4 col-lg-3-->                                    
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.GROUP_CODE' | translate }}</label>
                                        <h6 *ngIf="!editActivated">{{userRightsData.mgrpcdDesc | Custom}}</h6>
                                        <div *ngIf="editActivated">
											<select formControlName="mgrpcd">
                                                <option value="Select">Select [ 1009 ]</option>
                                                <option *ngFor="let data of groupCodeLookup" value="{{data.lookupComposite.mcode}}">{{data.mcodedesc}}</option>
											</select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['mgrpcd'].hasError('required') && (userAccessRightsForm.controls['mgrpcd'].dirty || userAccessRightsForm.controls['mgrpcd'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div><!--.col-12 col-md-4 col-lg-3-->                                        
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.USER_CODE' | translate }}</label>
                                        <h6 *ngIf="!editActivated">{{reportUsrName | Custom}}</h6>
                                        <div *ngIf="editActivated">
											<select formControlName="musrcode">
                                                <option value="Select">Select</option>
                                                <option *ngFor="let data of reportingUserDropdown" value="{{data.musrcode}}">{{data.musrName}}</option>
											</select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['musrcode'].hasError('required') && (userAccessRightsForm.controls['musrcode'].dirty || userAccessRightsForm.controls['musrcode'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div><!--.col-12 col-md-4 col-lg-3-->
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.AUTHORITY' | translate }}</label>
                                        <div *ngIf="!editActivated">
                                            <!-- <h6 *ngIf="!editActivated">{{userRightsData.authoritye | Custom}}</h6> -->
                                            <div *ngIf="userRightsData.authoritye == 1;then content else other_content"></div>    
                                            <ng-template #content><h6>Yes</h6></ng-template>
                                            <ng-template #other_content><h6>No</h6></ng-template>
                                        </div>
                                        <div *ngIf="editActivated">
                                            <select formControlName="authoritye">
                                                <option value="Select">Select</option>
                                                <option value="1">Yes</option>
                                                <option value="0">No</option>
                                            </select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['authoritye'].hasError('required') && (userAccessRightsForm.controls['authoritye'].dirty || userAccessRightsForm.controls['authoritye'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div><!--.col-12 col-md-4 col-lg-3-->
                                <div class="col-12 col-md-4 col-lg-3">                                                                   
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.BROWSE' | translate }}</label>
                                        <div *ngIf="!editActivated">
                                            <!-- <h6 *ngIf="!editActivated">{{userRightsData.browsee | Custom}}</h6> -->
                                            <div *ngIf="userRightsData.browsee == 1;then content else other_content"></div>    
                                            <ng-template #content><h6>Yes</h6></ng-template>
                                            <ng-template #other_content><h6>No</h6></ng-template>
                                        </div>
                                        <div *ngIf="editActivated">
                                            <select formControlName="browsee">
                                                <option value="Select">Select</option>
                                                <option value="1">Yes</option>
                                                <option value="0">No</option>
                                            </select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['browsee'].hasError('required') && (userAccessRightsForm.controls['browsee'].dirty || userAccessRightsForm.controls['browsee'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div>    
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.CREATE' | translate }}</label>
                                        <div *ngIf="!editActivated">
                                            <!-- <h6 *ngIf="!editActivated">{{userRightsData.createe | Custom}}</h6> -->
                                            <div *ngIf="userRightsData.createe == 1;then content else other_content"></div>    
                                            <ng-template #content><h6>Yes</h6></ng-template>
                                            <ng-template #other_content><h6>No</h6></ng-template>
                                        </div>
                                        <div *ngIf="editActivated">
                                            <select formControlName="createe">
                                                <option value="Select">Select</option>
                                                <option value="1">Yes</option>
                                                <option value="0">No</option>
                                            </select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['createe'].hasError('required') && (userAccessRightsForm.controls['createe'].dirty || userAccessRightsForm.controls['createe'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div>    
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.DELETE' | translate }}</label>
                                        <div *ngIf="!editActivated">
                                            <!-- <h6 *ngIf="!editActivated">{{userRightsData.deletee | Custom}}</h6> -->
                                            <div *ngIf="userRightsData.deletee == 1;then content else other_content"></div>    
                                            <ng-template #content><h6>Yes</h6></ng-template>
                                            <ng-template #other_content><h6>No</h6></ng-template>
                                        </div>
                                        <div *ngIf="editActivated">
                                            <select formControlName="deletee">
                                                <option value="Select">Select</option>
                                                <option value="1">Yes</option>
                                                <option value="0">No</option>
                                            </select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['deletee'].hasError('required') && (userAccessRightsForm.controls['deletee'].dirty || userAccessRightsForm.controls['deletee'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div>
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="{{formClass}}">
                                        <label>{{ 'HOME.MODIFY' | translate }}</label>
                                        <div *ngIf="!editActivated">
                                            <!-- <h6 *ngIf="!editActivated">{{userRightsData.modifye | Custom}}</h6> -->
                                            <div *ngIf="userRightsData.modifye == 1;then content else other_content"></div>    
                                            <ng-template #content><h6>Yes</h6></ng-template>
                                            <ng-template #other_content><h6>No</h6></ng-template>
                                        </div>
                                        <div *ngIf="editActivated">
                                            <select formControlName="modifye">
                                                <option value="Select">Select</option>
                                                <option value="1">Yes</option>
                                                <option value="0">No</option>
                                            </select><u></u>
                                        </div>
                                        <em *ngIf="userAccessRightsForm.controls['modifye'].hasError('required') && (userAccessRightsForm.controls['modifye'].dirty || userAccessRightsForm.controls['modifye'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-readcontroller-->
                                </div>                                        
                            </div>                        
                        </form>
                        <div class="row form-footer">
                            <div class="col-6" *ngIf="!editActivated">
                                <div class="text-left">
                                    <button class="linkbutton" routerLink="/user-access-rights-view">{{ 'ACTION.BACK' | translate }}</button>
                                </div>
                            </div>
                            <div class="col-6" *ngIf="editActivated">
                                <div class="text-left">
                                    <button class="linkbutton" (click)="backToView();">{{ 'ACTION.BACK' | translate }}</button>
                                </div>
                            </div>
                            <div *ngIf="editActivated" class="col-6">
                                <div class="text-right">
                                    <button class="button" (click)="onSubmit(userAccessRightsForm.value)">{{ 'ACTION.UPDATE' | translate }}</button>
                                </div>
                            </div>
                        </div><!--.row-->
                    </div><!--.in-mcontainer-->
                </div><!--.col-12-->
            </div><!--.row-->
        </div><!--.container-fluid-->
    </div><!--.in-main-->
</main><!--.main-container-->
<!-- ================ Main Page Content - End   ==============-->



