<div class="breadcrumb">
    <ul class="lst-breadcrumb">
        <!-- <li>
            <a href="javascript:;">Master</a>
        </li> -->
        <li>
            <a href="javascript:;" routerLink="/district-master-view">{{ 'HOME.DISTRICT_MASTER' | translate }}</a>
        </li>
        <li class="current">{{ 'ACTION.TEXT_ADD' | translate }}</li>
    </ul><!--.lst-breadcrumb-->
</div>
<div class="vspacer20"></div>
<main class="main-container">
    <div class="in-main">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="in-mcontainer">
                        <!--Alert Start-->
                        <div *ngIf="!code_exist" class="alert alert-danger code-exist-alert" role="alert">
                            {{code_exist_message}}
                        </div>
                        <!--Alert End-->
                        <div class="category">{{ 'HOME.DISTRICT_DETAILS' | translate }}</div>
                        <div class="vspacer20"></div>
                        <form [formGroup]="districtForm" (ngSubmit)="onSubmit(districtForm.value)">
                            <div class="row form-group">

                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="form-controller control-mandatory">
                                        <label>{{ 'HOME.STATE_CODE' | translate }}</label>
                                        <div>
											<select formControlName="mstatecd">
                                                <option value="Select">Select</option>
                                                <option *ngFor="let data of stateDropdownData" value="{{data.mstatecd}}">{{data.mstatedesc}}</option>
                                            </select><u></u>
                                        </div>
                                        <em *ngIf="districtForm.controls['mstatecd'].hasError('required') && (districtForm.controls['mstatecd'].dirty || districtForm.controls['mstatecd'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-control-->
                                </div>
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="form-controller control-mandatory">
                                        <label>{{ 'HOME.DISTRICT_CODE' | translate }}</label>
                                        <input type="text" formControlName="mdistcd" placeholder="Enter {{ 'HOME.DISTRICT_CODE' | translate }}"><u></u>
                                        <em *ngIf="districtForm.controls['mdistcd'].hasError('required') && (districtForm.controls['mdistcd'].dirty || districtForm.controls['mdistcd'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-control-->
                                </div>
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="form-controller control-mandatory">
                                        <label>{{ 'HOME.DISTRICT_NAME' | translate }}</label>
                                        <input type="text" formControlName="mdistdesc" placeholder="Enter {{ 'HOME.DISTRICT_NAME' | translate }}"><u></u>
                                        <em *ngIf="districtForm.controls['mdistdesc'].hasError('required') && (districtForm.controls['mdistdesc'].dirty || districtForm.controls['mdistdesc'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                    </div><!--.form-control-->
                                </div>
                                
                            </div>
                            
                            <div class="row">
                                <div class="col-6">
                                    <div class="text-left">
                                        <button class="linkbutton" routerLink="/district-master-view">{{ 'ACTION.CANCEL' | translate }}</button>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-footer text-right">
                                        <button class="button">{{ 'ACTION.SUBMIT' | translate }}</button>
                                    </div><!--.form-footer-->
                                </div><!--.col-12-->
                            </div><!--.row-->
                        </form>
                    </div><!--.in-mcontainer-->
                </div><!--.col-12-->
            </div><!--.row-->
        </div><!--.container-fluid-->
    </div><!--.in-main-->
</main><!--.main-container-->
<!-- ================ Main Page Content - End   ==============-->


