                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="form-controller control-mandatory">
                                        <label>{{ 'HOME.BRANCH_CODE' | translate }}</label>
                                        <!-- <input type="text" formControlName="mpbrcode" (keyup)="onKeyUp($event.target.value)" placeholder="{{ 'HOME.BRANCH_CODE' | translate }}"><u></u> -->
                                        <input type="text" formControlName="mpbrcode" placeholder="{{ 'HOME.BRANCH_CODE' | translate }}"><u></u>
                                        <em *ngIf="branchForm.controls['mpbrcode'].hasError('required') && (branchForm.controls['mpbrcode'].dirty || branchForm.controls['mpbrcode'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
                                        <em *ngIf="branchForm.controls['mpbrcode'].hasError('pattern') && (branchForm.controls['mpbrcode'].dirty || branchForm.controls['mpbrcode'].touched)">{{ 'VALIDATION.NUMBERS' | translate }}</em>
                                        <em *ngIf="branchForm.controls['mpbrcode'].hasError('maxlength') && (branchForm.controls['mpbrcode'].dirty || branchForm.controls['mpbrcode'].touched)">{{ 'VALIDATION.MAX_10' | translate }}</em>
                                        <!-- <em *ngIf="code_exist">{{ code_exist_message }}</em> -->
                                    </div><!--.form-control-->
                                </div>
                               
                                <div class="col-12 col-md-4 col-lg-3">
                                    <div class="form-controller control-mandatory">
