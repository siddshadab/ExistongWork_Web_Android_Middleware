                                        <em *ngIf="loanLvlForm.controls['morderid'].hasError('required') && (loanLvlForm.controls['morderid'].dirty || loanLvlForm.controls['morderid'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
