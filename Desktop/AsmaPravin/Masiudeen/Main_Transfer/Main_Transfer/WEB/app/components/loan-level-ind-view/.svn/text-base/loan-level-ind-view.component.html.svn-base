                                        <em *ngIf="loanLvlForm.controls['mprdcd'].hasError('required') && (loanLvlForm.controls['mprdcd'].dirty || loanLvlForm.controls['mprdcd'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
