                                        <input *ngIf="editActivated" type="text" formControlName="mismandatory" placeholder="{{ 'HOME.MANDATORY' | translate }}"><u></u> -->
                                        <div *ngIf="!editActivated">
                                            <!-- <h6 *ngIf="!editActivated">{{userRightsData.modifye | Custom}}</h6> -->
                                            <div *ngIf="loanLvlData.mismandatory == 1;then content else other_content"></div>    
                                            <ng-template #content><h6>Yes</h6></ng-template>
                                            <ng-template #other_content><h6>No</h6></ng-template>
                                        </div>
                                        <div *ngIf="editActivated">
                                            <select formControlName="mismandatory">
                                                <option value="Select">Select</option>
                                                <option value="1">Yes</option>
                                                <option value="0">No</option>
                                            </select><u></u>
