                                    </div>
                                </div>
                                <div class="col-6" *ngIf="editActivated">
                                    <div class="text-left">
                                        <button class="linkbutton" (click)="backToView();">{{ 'ACTION.BACK' | translate }}</button>
                                    </div>
                                </div>
                                <div *ngIf="editActivated" class="col-6">
                                    <div class="form-footer text-right">
                                        <button class="button" (click)="onSubmit(loanLvlForm.value)">{{ 'ACTION.UPDATE' | translate }}</button>
                                    </div>
                                </div>
