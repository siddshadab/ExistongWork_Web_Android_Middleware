                                        <h6 *ngIf="!editActivated">{{loanLvlData.mstageid | Custom}}</h6>
                                        <input *ngIf="editActivated" type="text" formControlName="mstageid" placeholder="{{ 'HOME.STAGE_ID' | translate }}"><u></u>
                                        <em *ngIf="loanLvlForm.controls['mstageid'].hasError('required') && (loanLvlForm.controls['mstageid'].dirty || loanLvlForm.controls['mstageid'].touched)">{{ 'VALIDATION.REQUIRED' | translate }}</em>
